
<%- include("partials/header") -%>
<h1>Write a New Post</h1>

<form class="" action="/compose" method="post">
  <div class="form-group" name="blogContent">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle">
    <label>Post</label>
    <textarea class="form-control" name="postBody" cols="40" rows="4"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>
